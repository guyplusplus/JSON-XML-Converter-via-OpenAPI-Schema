        "str1": {
          "type": "string"
        },
        "str2": {
          "type": "string"
        },
        "nb1": {
          "type": "number"
        }
      }
    },
    "anObject2": {
      "type": "object",
      "nullable": true,
      "properties": {
        "str1": {
          "type": "string"
        },
        "str2": {
          "type": "string"
        },
        "nb1": {
          "type": "number"
        }
      }
